<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="none">
  		<test name="RunTestCasesOnChrome">
  			<parameter name="browser" value="chrome"/>
  			<classes>
  				<class name="TestCore.CoreFile"></class>
  				<class name="TestCases.DoLoginCase"></class>
  				<class name="TestCases.SwitchLanguageCase"></class>
  				<class name="TestCases.RequiredTabAccessCase"></class>
  				<class name="TestCases.AccessClientInformationCase"></class>
  			</classes>
  		</test>
 	    <test name="RunTestCaseOnIE">
 			<parameter name="browser" value="ie"/>
  			<classes>
  				<class name="TestCore.CoreFile"></class>
  				<class name="TestCases.DoLoginCase"></class>
  				<class name="TestCases.SwitchLanguageCase"></class>
  				<class name="TestCases.RequiredTabAccessCase"></class>
  				<class name="TestCases.AccessClientInformationCase"></class>
  			</classes>
  		</test>
</suite>
